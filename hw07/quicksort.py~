def qSort(retL):
    if (retL == []):
        return retL

    # selects pivot point
    pivot = retL[0]

    # partition lower and upper lists
    lower = qSort([x for x in l[1:] if x < pivot])
    upper = qSort([x for x in l[1:] if x >= pivot])
    return lower + [pivot] + upper

print quickSort([5,2,7,1,9,13]) # -> [1,2,5,7,9,13]
print quickSort([8,31,23,96]) # -> [8,23,31,96]
print quickSort([-5,-7,9,15,8]) # -> [-7,-5,8,9,15]
